{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bunx biome lint ./src",
    "lint:fix": "bunx biome lint --write ./src",
    "format": "bunx biome format --write ./src",
    "format:check": "bunx biome format ./src",
    "biome:check": "bunx biome check ./src",
    "biome:fix": "bunx biome check --write ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/kit": "^2.47.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "svelte": "^5.41.4",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@tiptap/core": "^3.8.0",
    "@tiptap/extension-image": "^3.8.0",
    "@tiptap/extension-underline": "^3.8.0",
    "@tiptap/starter-kit": "^3.8.0",
    "lucide-svelte": "^0.545.0",
    "svelte-sonner": "^1.0.5"
  }
}
